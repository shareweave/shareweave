{
	"name": "frontend",
	"version": "0.0.1",
	"scripts": {
		"dev": "svelte-kit dev",
		"build": "svelte-kit build",
		"package": "svelte-kit package",
		"preview": "svelte-kit preview",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=.. . && eslint --ignore-path .gitignore .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=.. .",
		"storybook": "start-storybook -p 6006",
		"build-storybook": "build-storybook",
		"build-webcomponents": "cd src/lib/webcomponents && tailwindcss --postcss postcss.config.cjs -i input.css -o build/output.css --config tailwind.config.cjs && npx rollup --config rollup.config.js",
		"build-webcomponents:watch": "cd src/lib/webcomponents && tailwindcss --postcss postcss.config.cjs -i input.css -o build/output.css --config tailwind.config.cjs --watch & cd src/lib/webcomponents && npx rollup --config rollup.config.js --watch"
	},
	"devDependencies": {
		"@babel/core": "^7.17.0",
		"@datamodels/identity-profile-basic": "^0.1.2",
		"@iconify/svelte": "^2.1.2",
		"@rollup/plugin-commonjs": "^21.0.1",
		"@rollup/plugin-node-resolve": "^13.1.3",
		"@storybook/addon-actions": "^6.4.18",
		"@storybook/addon-essentials": "^6.4.18",
		"@storybook/addon-links": "^6.4.18",
		"@storybook/addon-svelte-csf": "^1.1.0",
		"@storybook/svelte": "^6.4.18",
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/adapter-static": "^1.0.0-next.26",
		"@sveltejs/kit": "next",
		"@typescript-eslint/eslint-plugin": "^4.31.1",
		"@typescript-eslint/parser": "^4.31.1",
		"autoprefixer": "^10.4.2",
		"babel-loader": "^8.2.3",
		"esifycss": "^1.4.37",
		"eslint": "^7.32.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-storybook": "^0.5.6",
		"eslint-plugin-svelte3": "^3.2.1",
		"node-sass": "^7.0.1",
		"postcss": "^8.4.6",
		"postcss-cli": "^9.1.0",
		"postcss-import": "^14.0.2",
		"prettier": "^2.5.1",
		"prettier-plugin-svelte": "^2.6.0",
		"prettier-plugin-tailwindcss": "^0.1.5",
		"rollup-plugin-css-only": "^3.1.0",
		"rollup-plugin-embed-css": "^1.0.24",
		"rollup-plugin-import-css": "^3.0.2",
		"rollup-plugin-livereload": "^2.0.5",
		"rollup-plugin-postcss": "^4.0.2",
		"rollup-plugin-svelte": "^7.1.0",
		"svelte": "^3.46.4",
		"svelte-check": "^2.2.6",
		"svelte-loader": "^3.1.2",
		"svelte-preprocess": "^4.9.4",
		"tailwindcss": "^3.0.15",
		"tslib": "^2.3.1",
		"typescript": "^4.4.3"
	},
	"type": "module",
	"dependencies": {
		"@3id/connect": "^0.2.5",
		"@datamodels/identity-profile-basic": "^0.1.2",
		"@self.id/web": "^0.2.0",
		"buffer": "^6.0.3",
		"dids": "^2.4.3",
		"micromodal": "^0.4.10",
		"svelte-simple-modal": "^1.3.0",
		"sveltejs-adapter-ipfs": "^0.2.1"
	}
}